{
    "apiVersion": "v1",
    "kind": "DeploymentConfig",
    "metadata": {
        "creationTimestamp": null,
        "generation": 1,
        "labels": {
            "app": "example-basicphp"
        },
        "name": "basicphp",
        "selfLink": "/oapi/v1/namespaces/examples/deploymentconfigs/basicphp"
    },
    "spec": {
        "replicas": 1,
        "selector": {
            "name": "basicphp"
        },
        "strategy": {
            "activeDeadlineSeconds": 21600,
            "resources": {},
            "rollingParams": {
                "intervalSeconds": 1,
                "maxSurge": "25%",
                "maxUnavailable": "25%",
                "timeoutSeconds": 600,
                "updatePeriodSeconds": 1
            },
            "type": "Rolling"
        },
        "template": {
            "metadata": {
                "creationTimestamp": null,
                "labels": {
                    "name": "basicphp"
                }
            },
            "spec": {
                "containers": [
                    {
                        "env": [
                            {
                                "name": "APPLICATION_NAME",
                                "value": "basicphp"
                            }
                        ],
                        "image": "factory:8081/docker-local/basicphp2:@VERSION@",
                        "imagePullPolicy": "Always",
                        "name": "basicphp",
                        "ports": [
                            {
                                "containerPort": 8080,
                                "name": "php",
                                "protocol": "TCP"
                            }
                        ],
                        "resources": {},
                        "terminationMessagePath": "/dev/termination-log",
                        "terminationMessagePolicy": "File"
                    }
                ],
                "dnsPolicy": "ClusterFirst",
                "restartPolicy": "Always",
                "schedulerName": "default-scheduler",
                "securityContext": {},
                "terminationGracePeriodSeconds": 30
            }
        },
        "test": false,
        "triggers": [
            {
                "type": "ConfigChange"
            }
        ]
    },
    "status": {
        "availableReplicas": 0,
        "latestVersion": 0,
        "observedGeneration": 0,
        "replicas": 0,
        "unavailableReplicas": 0,
        "updatedReplicas": 0
    }
}
